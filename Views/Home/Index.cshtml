<!DOCTYPE html>
<html lang="ru">
    <head>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>Webbr</title>
        <link rel="stylesheet" href="~/dist/main.css" asp-append-version="true" />
        @if (ViewBag.WebbrTheme == "dark") {
        <link rel="stylesheet" href="~/dist/bootstrapDark.css" id="webbr_theme" />
        } else if (ViewBag.WebbrTheme == "light") {
        <link rel="stylesheet" href="~/dist/bootstrapLight.css" id="webbr_theme" />
        }
        <link rel="stylesheet" href="~/dist/bootstrapVue.css" />
    </head>
    
    <body>
        <div id="app"></div>
        <script src="~/dist/main.js" asp-append-version="true"></script>
    </body>
</html>